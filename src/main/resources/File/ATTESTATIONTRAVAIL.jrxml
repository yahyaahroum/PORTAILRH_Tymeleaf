	<parameter name="matricule" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT TOP 1 CONTRAT,
(SELECT CIVILITE FROM PINDIVIDU WHERE DOS=1 AND INDIVIDU=LEFT(CONTRAT,5)) CIVILITE,
(SELECT CONCAT(RTRIM(NOMUSAGE),' ',RTRIM(PRENOM)) FROM PINDIVIDU WHERE DOS=1 AND INDIVIDU=LEFT(CONTRAT,5)) NOM,
(SELECT RTRIM(LIB) FROM PPTEMP WHERE EMPLOICOD=PCONT.EMPLOICOD) FONCTION,
(SELECT UP_CIN FROM PINDIVIDU WHERE DOS=1 AND INDIVIDU=LEFT(CONTRAT,5)) CIN,
(SELECT SECUNUM FROM PINDIVIDU WHERE DOS=1 AND INDIVIDU=LEFT(CONTRAT,5)) CNSS,
ENTREEDT,'Brut Mensuel'  PROFILCOD
FROM PCONT WHERE DOS=1 AND RTRIM(LEFT(CONTRAT,5)) LIKE  $P{matricule}
ORDER BY PCONT_ID DESC]]>
